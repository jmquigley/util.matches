{
  "name": "util.matches",
  "version": "0.0.5",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "Searches a string for N instances of a given regex.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.matches"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.matches/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "devDependencies": {
    "@types/node": "^8.0.54",
    "ava": "^0.24.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "debug": "^3.1.0",
    "nyc": "^11.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "util.pkgwrap": "^0.0.88"
  },
  "ava": {
    "babel": "inherit"
  },
  "pkgwrap": {
    "cleanup": [
      "index.d.ts",
      "index.js",
      "test.d.ts",
      "test.js"
    ]
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn test && yarn run report",
    "build": "yarn clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  }
}
